9 ST=PEEK(18)*65536+PEEK(19)*256+PEEK(20)
10 DIM N$(8192)
20 N$="0":N(8192)="0":N$(2:8192)=N$
30 FOR I=1 TO 8192:IF N$(I,I)="1" THEN 60
40 PRINT=I+I+1:CNT=CNT+1:K=I
50 K=K+PRIME:IF K<8193 THEN N$(K,K)="1":GOTO 50
60 NEXT I
70 PRINT CNT:REM BETTER PRINT 1899!!!
78 ET=PEEK(18)*65536+PEEK(19)*256+PEEK(20)
79 PRINT"E T=";(ET-ST)/60
